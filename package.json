{
  "name": "app-website",
  "version": "3.3.0",
  "private": true,
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "download:local:env": "aws s3 cp s3://case-consulting-website-code-dev/.env .env",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "lint": "npm run eslint && npm run stylelint",
    "precommit": "npm run validate && npm run lint && npm run stylelint && npm run unit",
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "dependencies": {
    "axios": "0.27.2",
    "bootstrap-vue": "2.23.1",
    "core-js": "3.30.2",
    "jquery": "3.6.4",
    "vue": "2.6.14",
    "vue-multiselect": "2.1.7",
    "vue-router": "3.5.4",
    "vue2-dropzone": "3.6.0",
    "vuelidate": "0.7.7",
    "vuepress": "1.9.9",
    "vuetify": "2.6.10",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@vue/cli-plugin-babel": "4.5.19",
    "@vue/cli-plugin-e2e-cypress": "4.5.19",
    "@vue/cli-plugin-unit-jest": "4.5.19",
    "@vue/cli-service": "4.5.19",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/test-utils": "1.3.5",
    "babel-jest": "27.5.1",
    "eslint": "8.40.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.11.0",
    "jshint": "2.13.6",
    "postcss": "8.4.23",
    "postcss-html": "1.5.0",
    "postcss-loader": "4.3.0",
    "precommit-hook": "3.0.0",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "sass-loader": "10.4.1",
    "stylelint": "15.6.1",
    "stylelint-config-recommended-scss": "11.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "vue-template-compiler": "2.6.14"
  },
  "pre-commit": [
    "validate",
    "lint",
    "unit"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}
