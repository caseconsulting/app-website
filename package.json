{
  "name": "app-website",
  "version": "3.3.0",
  "private": true,
  "scripts": {
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "clean": "rm -rf dist",
    "download:local:env": "aws s3 cp s3://case-consulting-website-code-dev/.env .env",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "lint": "npm run eslint && npm run stylelint",
    "precommit": "npm run validate && npm run eslint && npm run stylelint && npm run test",
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "axios": "0.27.2",
    "jquery": "3.7.1",
    "vue": "2.6.14",
    "vue-multiselect": "2.1.7",
    "vue-router": "3.5.4",
    "vue2-dropzone": "3.6.0",
    "vuelidate": "0.7.7",
    "vuetify": "2.6.10",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/eslint-parser": "7.22.15",
    "@vue/cli-plugin-babel": "4.5.19",
    "@vue/cli-plugin-e2e-cypress": "4.5.19",
    "@vue/cli-plugin-unit-jest": "4.5.19",
    "@vue/cli-service": "4.5.19",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/test-utils": "1.3.5",
    "babel-jest": "29.7.0",
    "eslint": "8.50.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "9.17.0",
    "postcss": "8.4.31",
    "postcss-html": "1.5.0",
    "postcss-loader": "4.3.0",
    "precommit-hook": "3.0.0",
    "prettier": "3.0.3",
    "sass": "1.68.0",
    "sass-loader": "10.4.1",
    "stylelint": "15.10.3",
    "stylelint-config-recommended-scss": "13.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "vue-template-compiler": "2.6.14"
  },
  "pre-commit": [
    "validate",
    "eslint",
    "stylelint",
    "test"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}
