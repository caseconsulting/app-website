{
  "name": "app-website",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "download:local:env": "aws s3 cp s3://case-consulting-website-code/.env .env",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "lint": "vue-cli-service lint --mode production",
    "precommit": "npm run validate && npm run lint && npm run stylelint && npm run unit",
    "serve": "vue-cli-service serve",
    "stylelint": "stylelint src/**/*.{css,scss,vue}",
    "test": "npm run test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "unit": "npm run test:unit",
    "validate": "npm ci"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.8.1",
    "jquery": "^3.5.1",
    "vue": "^2.6.12",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.4.9",
    "vue2-dropzone": "^3.6.0",
    "vuelidate": "^0.7.6",
    "vuepress": "^1.7.1",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.10.0",
    "fibers": "^5.0.0",
    "jshint": "^2.13.0",
    "node-sass": "^7.0.0",
    "precommit-hook": "^3.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.13.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^3.18.0",
    "vue-template-compiler": "^2.6.12"
  },
  "pre-commit": [
    "validate",
    "lint",
    "stylelint",
    "unit"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "eslint --fix src",
      "git add"
    ]
  }
}
